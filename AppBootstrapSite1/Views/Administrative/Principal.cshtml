@model AppBootstrapSite1.Models.Administrative 
@{
    ViewBag.Title = "Principal";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container marketing">


    <div class="row featurette">
        <div class="col-md-7">
            <h2 class="featurette-heading"><span class="text-muted">@Html.LabelFor(model => model.PrincipalHeading)</span></h2>
            <p>@Html.LabelFor(model => model.Principal, new { style = "color:#ffffff" })</p>
        </div>
        <div class="col-md-5">
            <img class="featurette-image img-responsive" src="/Content/abdur-razzak500x500.png" alt="Generic placeholder image">
        </div>
    </div>

    <hr class="featurette-divider">


</div> 

